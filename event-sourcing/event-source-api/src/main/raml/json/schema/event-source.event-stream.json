{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "data": {
      "id": "/properties/data",
      "items": {
        "id": "/properties/data/items",
        "properties": {
          "createdAt": {
            "id": "/properties/data/items/properties/createdAt",
            "type": "string",
            "format": "date-time"
          },
          "eventId": {
            "id": "/properties/data/items/properties/eventId",
            "type": "string"
          },
          "name": {
            "id": "/properties/data/items/properties/name",
            "type": "string"
          },
          "payload": {
            "id": "/properties/data/items/properties/payload",
            "type": "object"
          },
          "sequenceId": {
            "id": "/properties/data/items/properties/sequenceId",
            "type": "integer"
          },
          "streamId": {
            "id": "/properties/data/items/properties/streamId",
            "type": "string"
          }
        },
        "type": "object"
      },
      "type": "array"
    },
    "paging": {
      "id": "/properties/paging",
      "properties": {
        "next": {
          "id": "/properties/paging/properties/next",
          "type": "string",
          "format": "uri",
          "required": false
        },
        "previous": {
          "id": "/properties/paging/properties/previous",
          "type": "string",
          "format": "uri",
          "required": false
        }
      },
      "type": "object"
    }
  },
  "type": "object"
}